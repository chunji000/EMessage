create table base(
	bid int primary key auto_increment,
	pbid int,
	code varchar(10),	
	name varchar(100),
	dex int,
	des varchar(50)	
)auto_increment=76;

create table dept(
	did int primary key auto_increment,
	pdid int,
	tid int,
	bid int,
	name varchar(30),
	ress varchar(30),	
	tel varchar(30),
	tact varchar(30),	
	mob varchar(30)	
)auto_increment=83;

create table accs(
	aid int primary key auto_increment,
	did int,
	rid int,	
	username varchar(20),
	password varchar(20),
	sex varchar(10),	
	tel varchar(20),
	stat int,	
	job varchar(20),
	name varchar(20),
	em varchar(30),
	des varchar(100)
)auto_increment=95;

create table roles(
	rid int primary key auto_increment,
	name varchar(30),	
	des varchar(100)
) auto_increment=89;

create table roles_funs(
	rid int,
	fid int 
);

create table funs(
	fid int primary key auto_increment,
	pfid int,
	uri varchar(100),
	des varchar(100),
	icon varchar(50)     
)auto_increment=117;

create table sena(
	sid int primary key auto_increment,
	unit varchar(60),
	bid int,	
	code varchar(20),
	fade varchar(20),
	ress varchar(100),
	tel varchar(20),	
	tact varchar(20),
	job varchar(20),
	mob varchar(15),
	em varchar(30),	
	username varchar(20),
	password varchar(20),
	des varchar(100),
	stat int	
);


create table project(
	pid int primary key auto_increment,
	tid int,
	cid int,
	fdid int,	
	cdid int,
	bid int,
	vid int,
	sid int,
	aid int,
	stat int,
	kid int,
	mary varchar(100),
	name varchar(50),
	pno varchar(20),
	year varchar(4),
	ress varchar(100),
	lat varchar(15),
	lon varchar(15),
	star date,	
	en date,	
	total int,
	detail varchar(100),
	mon int,
	head varchar(50),
	mob varchar(50),
	des varchar(255),
	createrid int,  
	createtime date    
);

create table projectfile(
	pfid int primary key auto_increment,
	pid int,
	fn varchar(50),	
	txt longblob,	
	tp varchar(30),	
	siz int,	
	tim date	
);

create table money(
	pid int primary key auto_increment,
	total int,	
	cou int,	
	pro int,
	city int,	
	dist int,
	com int	
);

create table apply(
	pid int primary key auto_increment,
	aaid int,	
	did int,	
	lastid int,	
	curdid int,	
	wid int,	
	tim date
);


create table step(
	sid int primary key auto_increment,
	pid int,
	bid int,
	aid int,
	did int,
	nid int,
	tim date,
	txt varchar(200),
	tid int	
);

create table stepfile(
	sfid int primary key auto_increment,
	sid int,
	pid int,	
	fn varchar(50),	
	txt longblob,	
	tp varchar(30),	
	siz int,
	tim date
);

create table report(
	rid int primary key auto_increment,
	pid int,
	aid int,
	did int,
	wid int,
	tid int,	
	retim date,	
	des varchar(255),
	rea varchar(100),
	state int,
	num int,
   	fintim date	
);

create table item(
	iid int primary key auto_increment,
	pid int,	
	wid int,	
	tact varchar(20),
	dist varchar(100),
	dname varchar(50),
	tim date,	
	txt varchar(255)
);

create table itemfile(
	ifid int primary key auto_increment,
	iid int,
	pid int,
	fn varchar(50),
	txt blob,	
	tp varchar(30),	
	siz int,
	tim date	
);

create table water(
	wid int primary key auto_increment,
	pid int,
	cod int,
	am int,	
	tp int,	
	ph int,	
	des varchar(255),	
	tim date	
);

create table accept(
	pid int primary key auto_increment,
	aid int,
	did int,
	wid int,
	stat int,
	note varchar(255),
	tim date,	
	tel varchar(15),
	per varchar(20),
	noe varchar(255),
	work varchar(255),
	res varchar(255)
);

create table acceptfile(
	afid int primary key auto_increment,
	pid int,
	fn varchar(50),
	txt blob,	
	tp varchar(30),	
	siz int,
	tim date
);

create table one(
	oid int primary key auto_increment,
	pid int,
	tact varchar(20),
	mob varchar(15),
	tel varchar(15),
	job varchar(20),
	em varchar(30),	
	sue1 int,	
	sue2 int,
	sue21 int,	
	sue22 int,	
	sue23 int,
	sue3 int,
	sue4 int,
	sue5 int,
	sue6 int,
	goa1 int,
	goa2 int,	
	goa21 int,	
	goa22 int,
	goa23 int,
	goa3 int,
	goa4 int,	
	goa5 int,	
	goa6 int,	
	goa7 int,	
	an varchar(30),	
	fact varchar(30),	
	mas varchar(30),	
	dea1 int,
	dea2 int,
	dea3 int,	
	dea4 int,
	dea5 int,
	tjc varchar(30),
	yyc varchar(30),
	yyctim varchar(30),	
	bqc varchar(30),	
	bqctim varchar(30),	
	createtim date
);

create table manc(
	mid int primary key auto_increment,
	oid int,
	name varchar(50),
	val varchar(20),
	dex int      
);

create table four(
	fid int primary key auto_increment,
	pid int,
	site varchar(50),	
	vig varchar(50),
	build varchar(50),
	area varchar(20),
	hole varchar(20),	
	total varchar(20),
	cost varchar(20),
	fintim date,
	acctim date,
	accor varchar(20),
	atact varchar(30),
	suppor varchar(20),
	stact varchar(30),
	met varchar(100),
	info1 varchar(10),	
	info2 varchar(10),
	info3 varchar(100),	
	info4 varchar(100),	
	info5 varchar(100),	
	createtim date	
);

create table seven(
	vid int primary key auto_increment,
	pid int,
	site varchar(50),	
	vig varchar(50),
	build varchar(50),	
	tp varchar(20),
	siz varchar(30),	
	total varchar(20),	
	fintim date,	
	acctim date,
	accor varchar(20),	
	atact varchar(30),	
	suppor varchar(20),
	stact varchar(30),	
	met varchar(100),
	info1 varchar(20),
	info2 varchar(100),	
	info3 varchar(100),	
	createtim date	
);

create table three(
	tid int primary key auto_increment,
	pid int,
	site varchar(50),	
	vig varchar(50),	
	build varchar(50),
	area varchar(30),
	rest varchar(50),	
	total varchar(20),
	fintim date,
	acctim date,	
	acctor varchar(20),
	atact varchar(30),	
	suppor varchar(20),	
	stact varchar(30),	
	met varchar(100),	
	info1 varchar(100),	
	info2 varchar(100),	
	createtim date	
);

