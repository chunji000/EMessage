package com.tsinghua.esco.base.vo;
// Table: funs	权限表
public class VFunsInfo {
private Integer 	 fid ;//int primary key auto_increment,
private Integer 	 	pfid;// int,	--父权限ID，自身外键
private String  	uri;// varchar(100),	--访问地址
private String   	des;// varchar(100),	--描述
private String   	icon ;//varchar(50)        --菜单图标
public Integer getFid() {
	return fid;
}
public void setFid(Integer fid) {
	this.fid = fid;
}
public Integer getPfid() {
	return pfid;
}
public void setPfid(Integer pfid) {
	this.pfid = pfid;
}
public String getUri() {
	return uri;
}
public void setUri(String uri) {
	this.uri = uri;
}
public String getDes() {
	return des;
}
public void setDes(String des) {
	this.des = des;
}
public String getIcon() {
	return icon;
}
public void setIcon(String icon) {
	this.icon = icon;
}


}
